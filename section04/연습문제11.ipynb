    "# -*- coding: utf-8 -*-\n",
    "\n",
    "# 공배수의 합을 구하기 위한 변수\n",
    "sum=0\n",
    "\n",
    "# i에 1~100까지 순차적으로 저장하는 반복문\n",
    "for i in range(1,101):\n",
    "    if i%5==0 and i%3==0:\n",
    "        sum+=i\n",
    "        print(i)\n",
    "\n",
    "tpl = \"공배수의 총 합 : {0}\"\n",
    "print(tpl.format(sum))\n"
   ]
  }
